{
  "expo": {
    "name": "Croupier Assistant",
    "slug": "caapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-1.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-1.png",
      "resizeMode": "cover",
      "backgroundColor": "#fff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.g0rdonshumway.caapp",
      "infoPlist": {
        "NSCameraUsageDescription": "We need access to your camera",
        "NSPhotoLibraryUsageDescription": "We need access to your photo library"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-1.png",
        "backgroundColor": "#fff"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "package": "com.g0rdonshumway.caapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "c9357b8d-3388-4c47-85da-4fdbeb341049"
      }
    },
    "owner": "g0rdonshumway"
  }
}